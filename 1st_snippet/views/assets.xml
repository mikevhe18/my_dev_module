<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

        <template id="website_accordion_frontend" inherit_id="website.assets_frontend" name="Accordion assets">
            <xpath expr="." position="inside">      
                <script type="text/javascript" src="/1st_snippet/static/src/js/accordion.js"></script>          
            </xpath>
        </template>
        
        <template id="website_accordion_theme_assets" name="website assets for Accordion" inherit_id="website.theme">
            <xpath expr="." position="inside">
                <link href="/1st_snippet/static/src/css/website_accordion.css" type="text/css" rel="stylesheet" />
            </xpath>
        </template>

</data>
</openerp>
